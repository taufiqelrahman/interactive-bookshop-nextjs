  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(css|scss|json|md)": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "22.x"
  }
}
