{
  "name": "interactive-bookshop-nextjs",
  "version": "1.0.0",
  "description": "Customized children books.",
  "main": "index.js",
  "author": "taufiqelrahman",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "lint": "eslint --ext js,ts,jsx,tsx --fix --ignore-path .eslintignore .",
    "format": "prettier --write '**/*.{js,ts,tsx,css,scss,json,md}'",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "deploy": "pnpm run build && pnpm run start",
    "analyze": "ANALYZE=true pnpm run build",
    "test": "TZ=Asia/Jakarta jest --coverage --colors --config=jest.config.js -u --silent",
    "test:ci": "TZ=Asia/Jakarta jest --coverage --colors --config=jest.config.ci.js -u --silent",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3000"
  },
